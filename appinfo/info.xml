<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>folder_protection</id>
    <name>Folder Protection</name>
    <summary>Protect folders from being moved, copied, or deleted</summary>
    <description>Prevents specific folders from being moved, copied, or deleted to avoid server performance issues with large folders.</description>
    <version>0.1.0</version>
    <licence>agpl</licence>
    <author>Ricardo</author>
    <namespace>FolderProtection</namespace>
    <category>files</category>
    
    <dependencies>
        <nextcloud min-version="31" max-version="31"/>
    </dependencies>
    <commands>
        <command>OCA\FolderProtection\Command\ListProtected</command>
        <command>OCA\FolderProtection\Command\Protect</command>
        <command>OCA\FolderProtection\Command\Unprotect</command>
        <command>OCA\FolderProtection\Command\CheckProtection</command>
    </commands>
<settings>
        <admin>OCA\FolderProtection\Settings\AdminSettings</admin>
    </settings>


</info>
